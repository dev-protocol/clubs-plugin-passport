{
	"name": "@devprotocol/clubs-plugin-passport",
	"version": "0.1.0-beta.19",
	"type": "module",
	"description": "Passport plugin for clubs",
	"main": "dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.d.ts",
		"dist/*.js",
		"!**/*.test.*",
		"!dist/vitest.*",
		"src"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "vitest",
		"preview": "astro --root .preview/preview.ssr dev",
		"preview:static": "astro --root .preview/preview.static dev",
		"build": "rollup -c",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test --run && yarn prebuild && yarn build",
		"prepare": "husky",
		"typecheck": "astro check --root ./src && vue-tsc --noEmit --p tsconfig.vue.json"
	},
	"author": "Dev Protocol",
	"license": "PolyForm Perimeter License 1.0.0",
	"dependencies": {
		"@devprotocol/clubs-plugin-payments": "0.1.0",
		"@devprotocol/dev-kit": "8.7.0",
		"@devprotocol/util-ts": "4.0.0",
		"bignumber.js": "9.1.2",
		"nanoid": "^5.0.7",
		"ramda": "0.30.1",
		"redis": "^4.7.0",
		"sass": "1.80.7"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^3.18.0",
		"ethers": "^6.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.0",
		"@astrojs/svelte": "^5.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@devprotocol/clubs-core": "^3.22.1",
		"@eslint/js": "^9.14.0",
		"@rollup/plugin-typescript": "12.1.1",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dotenv": "8.2.3",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "22.9.0",
		"@types/ramda": "0.30.2",
		"@typescript-eslint/eslint-plugin": "8.14.0",
		"@typescript-eslint/parser": "8.14.0",
		"astro": "^4.0.0",
		"dotenv": "16.4.5",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-functional": "^7.1.0",
		"ethers": "6.13.4",
		"globals": "^15.12.0",
		"husky": "9.1.6",
		"prettier": "3.3.3",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.6.0",
		"rimraf": "6.0.1",
		"rollup": "4.25.0",
		"rollup-plugin-dts": "^6.1.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "3.4.14",
		"typescript": "5.6.3",
		"typescript-eslint": "^8.13.0",
		"vitest": "^2.0.0",
		"vue": "3.5.12",
		"vue-tsc": "^2.0.0"
	},
	"packageManager": "yarn@4.5.1"
}
